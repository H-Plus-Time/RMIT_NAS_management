{
  "entrypoint": "index.html",
  "shell": "src/nas-mgmt-app.html",
  "fragments": [
    "src/nas-howto.html",
    "src/nas-profile.html",
    "src/nas-offers.html",
    "src/nas-admin.html",
    "src/nas-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "keys.js",
    "bower_components/login-fire/*.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "service-worker": true,
    "bundle": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }]
}
